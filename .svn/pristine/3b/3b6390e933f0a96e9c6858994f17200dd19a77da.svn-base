/* ================================头部样式 */
/*================== 顶栏图 */
.header .top-ad{
    height:100px;
    width:100%;
    background-color:rgb(205, 18, 25);
}
.header .ad-img{
    height:100px;
    width: 1200px;
    margin:0 auto;
    background: url(../images/img5/advertis.jpg) no-repeat;
    background-size:1200px 100px;
}
.header .top{
    width:1200px;
    margin:0 auto;
    font-size:12px;
}
.clearfix:after{
    content:".";
    display:block;
    clear:both;
    height: 0;
    visibility:hidden;
} 
/* =====================欢迎登陆列 */
.header .top-info{
    width: 100%;
    background-color: rgb(252,252,252);
    border-bottom:1px solid #edeef0;
}
.header .left{
    width: 300px;
    height: 33px;
    float:left;
    line-height:33px;
    color:gray;
}

.header .top-info .right a{
    color:black;
}
.header .top-info .right .li-finally span{
    color:rgb(255, 120, 0);
}
.header .top-info .right li:hover > a{
    color:rgb(255, 120, 0);
}
.header .top-info .right .li-top:hover .right-list{
    display:block;
}
.header .right{
    float:right;
    width: 480px;
    height: 33px;
    line-height:33px;
}
.header .right li{
    float:left;
    width: 120px;
    height: 33px;
    text-align:center;
    position: relative;
}
.header .right .li1{
    background: url(../images/img5/user.png) no-repeat 12px 8px;
}
.header .right .li1:hover{
    background: url(../images/img5/user1.png) no-repeat 12px 8px;
}
.header .right .li2{
    background: url(../images/img5/购物车.png) no-repeat 12px 8px;
}
.header .right .li2:hover{
    background: url(../images/img5/购物车1.png) no-repeat 12px 8px;
}
.header .right .li3{
    background: url(../images/img5/心形.png) no-repeat 12px 8px;
}
.header .right .li3:hover{
    background: url(../images/img5/心形1.png) no-repeat 12px 8px;
}
/* ============================悬停出现在线客服 */
.header .right .li4 .telephone{
    width: 243px;
    background-color: white;
    position: absolute;
    top: 33px;
    left: -125px;
    border: 1px solid #edeef0;
    z-index: 1;
    display: none;
}
.header .right .li4:hover .telephone{
    display: block;
}
.header .right .li4 .mail{
    width: 219px;
    height: 128px;
    border-bottom: 1px solid #edeef0;
    margin:0 12px;
    padding-bottom: 5px;
    background: url(../images/img5/ingge-1.0.1.png) no-repeat 9px 16px;
    background-size:100px 100px;
}
.header .right .li4 .mail .contact{
    width: 135px;
    height: 67px;
    float:right;
    margin-top:62px; 
    margin-right:25px; 
}

.header .right .li4 .mail .p1{
    width: 80px;
    height: 30px;
    background-color:rgb(255, 120, 0);
    float:right;
    border-radius: 30px;
    font-size: 14px;
    line-height: 30px;
    color:white;
}
.header .right .li4 .mail .p2{
    width: 135px;
    height: 28px;
    line-height: 28px;
    float:left;
    color:gray;
    margin-top: 7px;
}
.header .right .li4 .mobile{
    width: 219px;
    height: 90px;
    /* background-color: black; */
    border-bottom: 1px solid #edeef0;
    margin: 0 12px; 
}
.header .right .li4 .mobile .contImg{
    width: 60px;
    height: 60px;
    float:left;
    padding: 16px 0;
}
.header .right .li4 .mobile .contTim{
    width: 159px;
    height: 56px;
    float:left;
    padding: 18px 0;

}
.header .right .li4 .mobile .contTim .p1{
    font-size: 16px;
    color: #ff6600;
    margin-bottom: -10px;
    font-weight:bold;
}
.header .right .li4 .wechat{
    width: 204px;
    height: 73px;
    margin: 0 12px;
    padding: 15px 0 15px 15px;
}
.header .right .li4 .wechat .left{
    width: 45px;
    height: 46px;
    margin-top: 20px;
    margin-right: 20px;
    float: left; 
}
.header .right .li4 .wechat .middle{
    width: 48px;
    height: 34px;
    margin-top: 20px; 
    float: left; 
    color:gray;
}
.header .right .li4 .wechat .p1{
    margin-bottom: -17px;
    margin-top: -5px;
}
.header .right .li4 .wechat .right{
    width: 79px;
    height: 79px;
    float: right;
}
.header .top-info .right-list{
    width: 120px;
    height: 124px;
    background-color: white;
    position: absolute;
    top: 33px;
    left: 0;
    display: none;
}
.header .top-info .right-list li{
    width: 103px;
    height: 31px;
    text-align: left;
    padding-left:15px;
    border:1px solid #edeef0;
}
.header .top-info .right-list li:hover a{
    color:rgb(255, 120, 0);
}
.header .right .img2{
    width: 14px;
    height: 15px;
    background-color:rgb(255, 120, 0);
    border-top-left-radius:50%; 
    border-top-right-radius:50%;
    border-bottom-right-radius:50%;
    position:absolute;
    top:1px;
    right:0;
}
.header .right .img2 div{
    color:white;
    line-height: 14px;
}
.header .right img{
    padding:1px;
    vertical-align: middle;
}
.header .login .search-list a{
    color:black;
}
.header .login .search-list a:hover{
    color: rgb(255, 120, 0);
}
/* ==================login搜索 */
.header .center{
    width: 1200px;
    height: 100px;
    margin:0 auto;
}
.header .center .login{
    width: 1200px;
    padding-top:20px;
    margin:0 auto;
}
.header .center .search-box{
    width: 560px;
    height: 52px;
    float:left;
    margin-top:18px;
    margin-left:10px;
}
.header .center .login-img1{
    width: 220px;
    height: 75px;
    float:left;
}
.header .center .login-img2{
    width: 180px;
    height: 70px;
    float:left;
}

.header .center .search-left{
    width: 460px;
    height: 32px;
    float:left;
    border:2px solid rgb(255, 120, 0);
}
.header .center .search-right{
    width: 90px;
    height: 36px;
    float:left;
    text-align:center;
    line-height:36px;
    color:white;
    font-size:16px;
    background-color:rgb(255, 120, 0);
    /* border:2px solid rgb(255, 120, 0); */
}
.header .center .search-right:hover{
    cursor:pointer;
}
.header .center .search-list li{
    float:left;
    margin-right:10px;
    font-size: 12px;
}
.header .center .search-img{
    width: 151px;
    height: 40px;
    float:right;
    margin-top:17px;
}
.header .center .search-img1{
    width: 58px;
    height: 40px;
    margin-right:14px;
    float:left;
    background:url(../images/img5/goods-icon1.jpg) no-repeat;
    position: relative;
}
.header .center .search-img1 img{
    position: absolute;
    top:37px;
    left:-70px;
    display: none;
}
.header .center .search-img1:hover img{
    display: block;
}
.header .center .search-img2{
    width: 75px;
    height: 40px;
    float:left;
    background:url(../images/img5/goods-icon1.jpg) no-repeat;
    background-position: -57px 0;
}
/* ===================商品列表 */
.header .bottom{
    width: 1200px;
    margin:0 auto;
}
.header .bottom .left{
    width: 210px;
    height: 40px;
    float:left;
    position: relative;
}
.header .bottom .right{
    width: 210px;
    height: 40px;
    float:left;
}
.header .bottom .bottom-left{
    width: 200px;
    height: 40px;
    padding-left:10px;
    line-height:40px;
    background-color:rgb(255, 120, 0);
    float:left;
    
}
.header .bottom .bottom-right{
    width: 728px;
    float:left;
}
.header .bottom .left a{
    color:white;
}
.header .bottom .bottom-right a{
    color:black;
    font-weight:bold;
}
/*======================================= 放轮播图样式 */
.header .bottom .left .left-list a:hover{
    color: #ff6600;
}
.header .bottom .right a:hover{
    color: #ff6600;
}
.header .bottom .bottom-right li{
    float:left;
    width: 56px;
    height: 40px;
    line-height: 40px;
    margin:0 50px;
}
.header .bottom .left-list{
    width: 210px;
    height: 450px;
    float:left;
    background-color: rgba(0, 0, 0, 0.8);
    position: absolute;
    top:40px;
    left:0;
    display:none;
}
.header .bottom .left-list li{
    width: 195px;
    height: 53px;
    padding-left:15px;
    padding-top:10px;
    border-bottom: 1px solid gray;
}
.header .bottom .list-top{
    width: 195px;
    height: 21px;
    line-height: 21px;
}
.header .bottom .list-bottom{
    width: 195px;
    height: 23px;
    line-height: 23px;
    
}
.header .bottom .list-bottom a{
    margin-right:5px;
    color:gray;
}
.header .bottom .left-list .li-bottom{
    border-bottom:1px solid transparent;
}
/*=======================================
=========================================== 网页主题 */
/* 第一块全图 */
.content .content-img{
    width: 100%;
    background-color:rgb(245, 42, 153);

}
.content .content-img .a_img{
    height: 370px;
    width: 1340px;
    margin:0 auto;
    background:rgb(245, 42, 135) url(../images/img5/a_02-01.jpg) no-repeat 0 0 ;
    background-position: -362px 0; 
}
/* 第二块优惠卷 */
.content .discounts{
    width: 100%;
    background-color: rgb(245, 188, 39);
}
.content .discounts .discounts-box{
    width: 1200px;
    margin:0 auto;
    padding: 20px 0;
}
.content .discounts .discounts-img{
    width: 844px;
    margin:0 auto;
}
.content .discounts .discounts-img li{
    width: 211px;
    height: 138px;
    float:left;
}
.content .discounts .discounts-box p{
    height: 23px;
    line-height: 23px;
    text-align:center;
    padding-top: 15px;
    color:red;
}
/* 第三块类别 */
.content .category{
    width: 100%;
    background-color:rgb(152, 0, 53);
}
.content .category .list-box{
    width: 1200px;
    margin:0 auto;
    padding-top: 15px;
}
.content .category .list{
    height: 147px;
    width: 992px;
    margin: 0 auto;
}
.content .category li{
    height: 129px;
    width: 238px;
    float:left;
    margin: 0 5px;
}
/* 热卖产品 */
.content .hot{
    width: 100%;
    background-color: rgb(150, 42, 231)
}
.content .hot .hot-box{
    width: 1200px;
    margin:0 auto;
}
.content .hot .hot-title{
    width: 268px;
    height: 94px;
    margin:0 auto;
}
.content .hot .hot-box .left{
    width: 299px;
}
.content .hot .hot-box .left div{
    width: 299px;
    height: 195px;
}
.content .hot .hot-box .left-top{
    margin-bottom: 7px; 
}





