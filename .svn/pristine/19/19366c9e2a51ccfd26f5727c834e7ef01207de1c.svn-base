.header .top-ad{
    height:100px;
    width:100%;
    background-color:rgb(205, 18, 25);
}
.header .ad-img{
    height:100px;
    width: 1200px;
    margin:0 auto;
    background: url(../images/img5/advertis.jpg) no-repeat;
    background-size:1200px 100px;
}
.header .top{
    width:1200px;
    margin:0 auto;
    font-size:12px;
}
.clearfix:after{
    content:".";
    display:block;
    clear:both;
    height: 0;
    visibility:hidden;
} 
.header .top-info{
    width: 100%;
    background-color: rgb(252,252,252);
    border-bottom:1px solid gray;
}
.header .left{
    width: 300px;
    height: 33px;
    float:left;
    line-height:33px;
    color:gray;
    
}
.header .top-info .right a{
    color:black;
}
.header .top-info .right a:hover{
    color:rgb(255, 120, 0);
}
.header .right{
    float:right;
    width: 480px;
    height: 33px;
    line-height:33px;
}
.header .right li{
    float:left;
    width: 120px;
    height: 37px;
    text-align:center;
    position: relative;
}
.header .right .img1{
    padding:2px;
    vertical-align: middle;
}
.header .right .img2{
    width: 14px;
    height: 15px;
    background-color:rgb(255, 120, 0);
    border-top-left-radius:50%; 
    border-top-right-radius:50%;
    border-bottom-right-radius:50%;
    position:absolute;
    top:1px;
    right:0;
}
.header .right .img2 div{
    color:white;
    line-height: 14px;
}
.header .right img{
    padding:1px;
    vertical-align: middle;
}
.header .login .search-list a{
    color:black;
}
.header .login .search-list a:hover{
    color: rgb(255, 120, 0);
}

.header .center{
    width: 1200px;
    height: 100px;
    margin:0 auto;
}
.header .center .login{
    width: 1200px;
    padding-top:20px;
    margin:0 auto;
}
.header .center .search-box{
    width: 560px;
    height: 52px;
    float:left;
    margin-top:18px;
    margin-left:10px;
}
.header .center .login-img1{
    width: 220px;
    height: 75px;
    float:left;
}
.header .center .login-img2{
    width: 180px;
    height: 70px;
    float:left;
}

.header .center .search-left{
    width: 460px;
    height: 32px;
    float:left;
    border:2px solid rgb(255, 120, 0);
}
.header .center .search-right{
    width: 90px;
    height: 36px;
    float:left;
    text-align:center;
    line-height:36px;
    color:white;
    font-size:16px;
    background-color:rgb(255, 120, 0);
    /* border:2px solid rgb(255, 120, 0); */
}
.header .center .search-list li{
    float:left;
    margin-right:10px;
    font-size: 12px;
}
.header .center .search-img{
    width: 151px;
    height: 40px;
    float:right;
    margin-top:17px;
}
.header .center .search-img1{
    width: 58px;
    height: 40px;
    margin-right:14px;
    float:left;
    background:url(../images/img5/goods-icon1.jpg) no-repeat;
}
.header .center .search-img2{
    width: 75px;
    height: 40px;
    float:left;
    background:url(../images/img5/goods-icon1.jpg) no-repeat;
    background-position: -57px 0;
}
.header .bottom{
    width: 1200px;
    margin:0 auto;
}
.header .bottom .left{
    width: 210px;
    height: 40px;
    float:left;
}
.header .bottom .right{
    width: 210px;
    height: 40px;
    float:left;
}
.header .bottom .bottom-left{
    width: 200px;
    height: 40px;
    padding-left:10px;
    line-height:40px;
    background-color:rgb(255, 120, 0);
    float:left;
}
.header .bottom .bottom-right{
    width: 728px;
    float:left;
}
.header .bottom .left a{
    color:white;
}
.header .bottom .bottom-right a{
    color:black;
    font-weight:bold;
}
.header .bottom .left .left-list a:hover{
    color: #ff6600;
}
.header .bottom .right a:hover{
    color: #ff6600;
}
.header .bottom .bottom-right li{
    float:left;
    width: 56px;
    height: 40px;
    line-height: 40px;
    margin:0 50px;
}
.header .bottom .left-list{
    width: 210px;
    height: 450px;
    float:left;
    background-color: rgba(0, 0, 0, 0.8);
}
.header .bottom .left-list li{
    width: 195px;
    height: 53px;
    padding-left:15px;
    padding-top:10px;
    border-bottom: 1px solid gray;
}
.header .bottom .list-top{
    width: 195px;
    height: 21px;
    line-height: 21px;
}
.header .bottom .list-bottom{
    width: 195px;
    height: 23px;
    line-height: 23px;
    
}
.header .bottom .list-bottom a{
    margin-right:5px;
    color:gray;
}
.header .bottom .left-list .li-bottom{
    border-bottom:1px solid transparent;
}










